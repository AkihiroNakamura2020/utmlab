{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Event",
  "type": "object",
  "required": ["eventId","type","severity","subject","trace"],
  "properties": {
    "eventId":{"type":"string"},
    "type":{"type":"string"},
    "severity":{"type":"string","enum":["LOW","MEDIUM","HIGH","CRITICAL"]},
    "subject":{"type":"object"},
    "advice":{"type":"object"},
    "trace":{"type":"array","items":{"type":"object"}}
  }
}
